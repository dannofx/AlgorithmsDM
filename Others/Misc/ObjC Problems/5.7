#import "Solution.h"


@implementation Solution

- (int)swapBits:(int)number {
    int mask = 0b01;
    int count = 2;
    while (count < 32) {
        mask = mask | mask << count;
        count *= 2;
    }
    return ((number & mask) << 1) | ((number & ~mask) >> 1);
}

- (void)run {
}

@end
